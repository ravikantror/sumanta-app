
<h1>New Post</h1>
 
<%= form_for :article, :url=>{'action'=>'create'}, method: :post do |f| %>
 
 
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :des %><br>
    <%= f.text_area :des %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
 
<%end%>
 
<%= link_to "Back", articles_path %>
<%= link_to "Home", home_path %>
<%= link_to "All User Post", articles_path %>

